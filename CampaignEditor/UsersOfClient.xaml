<Window x:Class="CampaignEditor.UsersOfClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CampaignEditor"
        mc:Ignorable="d"
        Title="UsersOfClient" Height="450" Width="800">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto" Width="600" Height="400">
            <StackPanel x:Name="spUsers"                
                        CanVerticallyScroll="True">
                <local:UsersListItem/>
            </StackPanel>
        </ScrollViewer>

    </Grid>
</Window>
